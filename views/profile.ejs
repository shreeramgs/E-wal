<!DOCTYPE html>
<html>
  <head>
    <!--Import Google Icon Font-->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

    <!-- Compiled and minified CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">

    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <style>
      #header, #main, #footer {
        padding-left: 300px;
      }
      @media only screen and (max-width : 992px) {
        #header, #main, #footer {
          padding-left: 0;
        }
      }
    </style>
  </head>
  <body>

    <!--Nav Bar-->
    <div id="header" class="navbar-fixed">
      <nav>
        <div class="nav-wrapper grey darken-3">
          <a href="/das" class="brand-logo center">E-wal</a>
          <a href="#" data-target="mobile-demo" class="sidenav-trigger"><i class="material-icons">menu</i></a>
          <ul class="right hide-on-med-and-down">
            <li><a href="/das">Home</a></li>
          </ul>
        </div>
      </nav>
    </div>

    <ul class="sidenav #ffffff white sidenav-fixed" id="mobile-demo">
      <li b><a class="black-text" href="/das">Home<i class="material-icons black-text">home</i></a></li>
      <li class="divider nogap"></li>
      <li><a href="/profile" class="black-text">Profile<i class="material-icons black-text">account_circle</i></a></li>
      <li><a href="/wallet" class="black-text">wallet<i class="material-icons black-text">account_balance_wallet</i></a></li>
      <li><a href="/survey1" class="black-text">Survey<i class="material-icons black-text">account_balance</i></a></li>
      <li><a href="/pay" class="black-text">PAY<i class="material-icons black-text">payment</i></a></li>
      <li><a href="/out" class="black-text">LOGOUT<i class="material-icons black-text">power_settings_new</i></a></li>
        <li><a href="#" class="black-text">FAQ/Help<i class="material-icons black-text">help_outline</i></a></li>
        <li><a href="/remove" class="black-text">REMOVE ACCOUNT<i class="material-icons black-text">remove</i></a></li>
    </ul>

    <div class="fixed-action-btn">
      <a class="btn-floating btn-large red">
        <i class="large material-icons">add</i>
      </a>
      <ul>
        <li><a class="btn-floating red" href="/profilel"><i class="material-icons">account_circle</i></a></li>
        <li><a class="btn-floating #8e24aa purple darken-1" href="/wallet"><i class="material-icons">account_balance_wallet</i></a></li>
        <li><a class="btn-floating green" href="/bank"><i class="material-icons">account_balance</i></a></li>
        <li><a class="btn-floating blue" href="/pay"><i class="material-icons">payment</i></a></li>
        <li><a class="btn-floating blue" href="/#"><i class="material-icons">help_outline</i></a></li>
      </ul>
    </div>

    <!--Section-->
    <section id="main" style="background-image: url('img8.jpg'); height: 750px;">
      
        <div class="container row">
          <div class="col s10">
            <div class="card white">
              <div class="card-content">
                <span class="cart-title center"><h2>PROFILE</h2></span>
                <li class="divider nogap"></li>
                <form id="profile">
                    <div class="row">                   
                      <div class="input-field col s10">                 
                        <i class="material-icons prefix">stars</i>
                        <input id="UserName" type="text" name="UserName" disabled value="<%= user.userID %>">
                        <label for="UserName">User Id</label> 
                      </div>                      
                    </div>
  
                    <div class="row">
                      <div class="input-field col s10">             
                        <i class="material-icons prefix">account_circle</i>
                        <input id="Name" type="Text" name="Name" value="<%= user.name %>">
                        <label for="Name">Name</label>
                                               
                      </div>
                    </div>
  
                     <div class="row">
                       <div class="input-field col s10">                      
                        <i class="material-icons prefix">dialpad</i>
                        <input id="Number" type="number" name="Number" value="<%= user.mobile %>">
                        <label for="Number">Number</label>                     
                       </div>
                     </div>
  
                     <div class="row">
                       <div class="input-field col s10">                   
                          <i class="material-icons prefix">mail</i>
                          <input id="Email" type="email" name="Email" disabled value="<%= user.UserName %>">
                          <label for="Email">Email</label>              
                       </div>                    
                     </div>

                     <button class="btn waves-effect waves-light" type="submit" name="action" >
                      MAKE CHANGES
                        <i class="material-icons right">send</i>
                      </button>
                  </div>
                </form>
              </div>
            </div>   
          </div>
        </div>  
  
    </section>    
    <!--Footer-->
    <footer id="footer" class="page-footer grey darken-3">
      <div class="container">
        <div class="row">
          <div class="col l6 s12">
            <h5 class="white-text">Footer Content</h5>
            <p class="grey-text text-lighten-4">Welcome to your profile page! Here, you can manage and update your personal information and customize your preferences.</p>
          </div>
          <div class="col l4 offset-l2 s12">
            <h5 class="white-text">Links</h5>
            <ul>
              <li><a class="grey-text text-lighten-3" href="/das">Home</a></li>
            </ul>
          </div>
        </div>
      </div>
      <div class="footer-copyright grey darken-4">
        <div class="container">
          Â© 2018 Copyright Text
          <a class="grey-text text-lighten-4 right" href="#!">More Links</a>
        </div>
      </div>
    </footer>


    <!--JavaScript at end of body for optimized loading-->
    <!-- Compiled and minified JavaScript -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>

    <script>
      document.addEventListener('DOMContentLoaded', function() {
        var elems = document.querySelectorAll('.sidenav');
        var instances = M.Sidenav.init(elems, {});
        
        var elems = document.querySelectorAll('.fixed-action-btn');
        var instances = M.FloatingActionButton.init(elems,{});
      });
    </script>
    
  </body>
</html>